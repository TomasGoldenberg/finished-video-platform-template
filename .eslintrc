{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": ["airbnb"],
    "globals": {
      "document": false,
      "escape": false,
      "navigator": false,
      "unescape": false,
      "window": false,
      "describe": true,
      "before": true,
      "it": true,
      "expect": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "array-bracket-spacing": 0,
      "arrow-body-style": 0,
      "block-scoped-var": 0,
      "brace-style": [0, "1tbs", { "allowSingleLine": true }],
      "comma-spacing": [0, { "before": false, "after": true }],
      "comma-style": [0, "last"],
      "complexity": 0,
      "consistent-return": 1,
      "consistent-this": 0,
      "curly": [0, "multi-line"],
      "default-case": 0,
      "dot-location": [0, "property"],
      "dot-notation": 0,
      "eol-last": 0,
      "eqeqeq": [0, "allow-null"],
      "func-names": 0,
      "func-style": 0,
      "generator-star-spacing": [0, "both"],
      "guard-for-in": 0,
      "handle-callback-err": [0, "^(err|error|anySpecificError)$" ],
      "indent": [0, 0, { "SwitchCase": 1 }],
      "key-spacing": [0, { "beforeColon": false, "afterColon": true }],
      "linebreak-style": 0,
      "max-depth": 0,
      "max-len": [0, 1550, 4],
      "max-nested-callbacks": 0,
      "max-params": 0,
      "max-statements": 0,
      "new-cap": [0, { "newIsCap": true, "capIsNew": false }],
      "newline-after-var": [0, "never"],
      "new-parens": 0,
      "no-alert": 0,
      "no-array-constructor": 0,
      "no-bitwise": 0,
      "no-caller": 0,
      "no-catch-shadow": 0,
      "no-cond-assign": 0,
      "no-console": 0,
      "no-constant-condition": 0,
      "no-continue": 0,
      "no-control-regex": 0,
      "no-debugger": 0,
      "no-delete-var": 0,
      "no-div-regex": 0,
      "no-dupe-args": 0,
      "no-dupe-keys": 0,
      "no-duplicate-case": 0,
      "no-else-return": 0,
      "no-empty": 0,
      "no-empty-character-class": 0,
      "no-labels": 0,
      "no-eq-null": 0,
      "no-eval": 0,
      "no-ex-assign": 0,
      "no-extend-native": 0,
      "no-extra-bind": 0,
      "no-extra-boolean-cast": 0,
      "no-extra-parens": 0,
      "no-extra-semi": 0,
      "no-extra-strict": 0,
      "no-fallthrough": 0,
      "no-floating-decimal": 0,
      "no-func-assign": 0,
      "no-implied-eval": 0,
      "no-inline-comments": 0,
      "no-inner-declarations": [0, "functions"],
      "no-invalid-regexp": 0,
      "no-irregular-whitespace": 0,
      "no-iterator": 0,
      "no-label-var": 0,
      "no-lone-blocks": 0,
      "no-lonely-if": 0,
      "no-loop-func": 0,
      "no-mixed-requires": 0,
      "no-mixed-spaces-and-tabs": [0, false],
      "no-multi-spaces": 0,
      "no-multi-str": 0,
      "no-multiple-empty-lines": [0, { "max": 1 }],
      "no-native-reassign": 0,
      "no-negated-in-lhs": 0,
      "no-nested-ternary": 0,
      "no-new": 0,
      "no-new-func": 0,
      "no-new-object": 0,
      "no-new-require": 0,
      "no-new-wrappers": 0,
      "no-obj-calls": 0,
      "no-octal": 0,
      "no-octal-escape": 0,
      "no-path-concat": 0,
      "no-plusplus": 0,
      "no-process-env": 0,
      "no-process-exit": 0,
      "no-proto": 0,
      "no-redeclare": 0,
      "no-regex-spaces": 0,
      "no-reserved-keys": 0,
      "no-restricted-modules": 0,
      "no-script-url": 0,
      "no-self-compare": 0,
      "no-sequences": 0,
      "no-shadow": 0,
      "no-shadow-restricted-names": 0,
      "no-spaced-func": 0,
      "no-sparse-arrays": 0,
      "no-sync": 0,
      "no-ternary": 0,
      "no-throw-literal": 0,
      "no-trailing-spaces": 0,
      "no-undef": 0,
      "no-undef-init": 0,
      "no-undefined": 0,
      "no-underscore-dangle": 0,
      "no-unneeded-ternary": 0,
      "no-unreachable": 0,
      "no-unused-expressions": 0,
      "no-unused-vars": [0, { "vars": "all", "args": "none" }],
      "no-var": 0,
      "no-void": 0,
      "no-warning-comments": 0,
      "no-with": 0,
      "object-curly-newline": 0,
      "object-curly-spacing": [0, "always"],
      "one-var": 0,
      "operator-assignment": 0,
      "operator-linebreak": [0, "after"],
      "padded-blocks": 0,
      "prefer-const": 0,
      "quote-props": 0,
      "quotes": [0, "single", "avoid-escape"],
      "radix": 0,
      "jsx-quotes": [0, "prefer-single"],
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/media-has-caption": 0,
      "react/display-name": 0,
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-curly-spacing": [0, "never"],
      "react/jsx-equals-spacing": [0, "never"],
      "react/jsx-indent-props": [0, 0],
      "react/jsx-no-bind": [0, { "allowArrowFunctions": true }],
      "react/jsx-no-undef": 0,
      "react/jsx-pascal-case": 0,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 0,
      "react/jsx-uses-vars": 0,
      "react/no-did-mount-set-state": 0,
      "react/no-did-update-set-state": 0,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
      "react/forbid-prop-types": 0,
      "react/prefer-stateless-function": 0,
      "react/require-default-props": 0,
      "react/react-in-jsx-scope": 0,
      "react/self-closing-comp": 0,
      "react/sort-comp": 0,
      "react/jsx-wrap-multilines": 0,
      "semi-spacing": 0,
      "sort-vars": 0,
      "space-before-blocks": [0, "always"],
      "space-before-function-paren": [0, {"anonymous": "always", "named": "never"}],
      "space-in-parens": [0, "never"],
      "space-infix-ops": 0,
      "keyword-spacing": 0,
      "space-unary-ops": [0, { "words": true, "nonwords": false }],
      "spaced-comment": [0, "always"],
      "strict": 0,
      "use-isnan": 0,
      "valid-jsdoc": 0,
      "valid-typeof": 0,
      "vars-on-top": 0,
      "wrap-iife": [0, "any"],
      "wrap-regex": 0,
      "yoda": [0, "never"]
    }
  }